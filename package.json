{
  "name": "digitalpictureframe",
  "version": "7.5.2",
  "description": "Digital pictureframe",
  "main": ".\\src\\main\\main.js",
  "scripts": {
    "start": "electron .",
    "rebuild": ".\\node_modules\\.bin\\electron-rebuild.cmd -f -m ./node_modules/better-sqlite3",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "keywords": [
    "picture frame"
  ],
  "author": "Ppack",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "electron": "^37.2.3",
    "electron-builder": "^26.0.12"
  },
  "homepage": "",
  "dependencies": {
    "better-sqlite3": "^11.1.2",
    "exiftool-vendored": "^29.2.0",
    "express": "^4.18.2",
    "sherpa-onnx-node": "^1.12.18"
  },
  "build": {
    "appId": "com.picframe.quickstart",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": false,
      "runAfterFinish": false,
      "installerIcon": "./build/icon.ico"
    },
    "asarUnpack": [
      "src/common/**",
      "src/renderer/assets/**",
      "src/renderer/webApp/**",
      "db/**"
    ]
  }
}
